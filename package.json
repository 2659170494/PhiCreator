{
  "name": "phi-creator",
  "version": "0.2.1",
  "description": "Chart editor for PhiX",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phi-x/phi-creator.git"
  },
  "author": "PhiX Dev Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phi-x/phi-creator/issues"
  },
  "keywords": [
    "phiformat",
    "phigros",
    "phix"
  ],
  "homepage": "https://github.com/phi-x/phi-creator#readme",
  "main": "dist/source/main/index.cjs.js",
  "scripts": {
    "buildEnvTypes": "node ./bin/buildEnvTypes.js",
    "postinstall": "run-s buildEnvTypes",
    "prebuild": "run-s buildEnvTypes",
    "build": "node ./bin/build.js",
    "precompile": "cross-env MODE=production run-s build",
    "compile": "electron-builder build --config ./config/electron-builder.js",
    "compile:only": "electron-builder build --config ./config/electron-builder.js",
    "prewatch": "run-s buildEnvTypes",
    "watch": "node ./bin/watch.js",
    "lint": "eslint --cache --ext js,ts,tsx .",
    "pretypecheck": "run-s buildEnvTypes",
    "typecheck:main": "tsc --noEmit -p ./src/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p ./src/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p ./src/renderer/tsconfig.json",
    "typecheck": "run-p typecheck:*"
  },
  "devDependencies": {
    "@types/css-font-loading-module": "^0.0.4",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-json-storage": "^4.0.0",
    "@types/fs-extra": "^9.0.9",
    "@types/howler": "^2.2.2",
    "@types/lodash-es": "^4.17.4",
    "@types/object-hash": "^2.1.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vitejs/plugin-react-refresh": "^1.3.2",
    "chokidar": "^3.5.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "vite": "^2.1.5"
  },
  "dependencies": {
    "@inlet/react-pixi": "^6.5.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@pixi-essentials/cull": "^1.1.0",
    "@pixi/unsafe-eval": "^6.0.1",
    "@rooks/use-boundingclientrect": "^4.10.1",
    "@rooks/use-on-window-resize": "^4.10.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.4",
    "electron-json-storage": "^4.4.0",
    "electron-updater": "^4.3.8",
    "electron-util": "^0.14.2",
    "fs-extra": "^9.1.0",
    "howler": "^2.2.1",
    "lodash-es": "^4.17.21",
    "mobx": "^6.1.8",
    "mobx-react-lite": "^3.2.0",
    "mobx-state-tree": "^5.0.1",
    "mst-middlewares": "^5.0.1",
    "notistack": "^1.0.5",
    "object-hash": "^2.1.1",
    "pixi-particles": "^4.3.0",
    "pixi.js": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
